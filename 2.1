func searchMaxandMin<M:Comparable>(num: [M]) -> (max:M,min:M) {

var max = num[0]

var min = num[0]


for i in num{

   if max < i {

      max = i

    }

         
   else if min > i{

      min = i
 
     }

     
    }

    return (max,min)

    
}


let tempInt = searchMaxandMin(num:[1,2,3])

print("传入整数时最大值为：\(tempInt.max) 最小值为：\(tempInt.min)")


let tempFloat = searchMaxandMin(num:[12.0,13.5,7.0])

print("传入浮点数时最大值为：\(tempFloat.max) 最小值为\(tempFloat.min)")

let tempString = searchMaxandMin(num:["A","B","C"])

print("传入字符串时最大值为：\(tempString.max) 最小值为\(tempString.min)")
